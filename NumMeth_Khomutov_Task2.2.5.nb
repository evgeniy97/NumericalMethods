     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", 
         RowBox[{"x2", "-", "x1"}], "]"}], "\[GreaterEqual]", "epselon"}], 
       ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x1", "=", "x2"}], ";", "\[IndentingNewLine]", 
         RowBox[{"x2", "=", 
          RowBox[{"x1", "-", 
           RowBox[{
            RowBox[{"f", "[", "x1", "]"}], "/", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{"f", "[", "x", "]"}], ",", "x"}], "]"}], "/.", 
              RowBox[{"x", "\[Rule]", "x1"}]}], ")"}]}]}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"n", "++"}], ";"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
