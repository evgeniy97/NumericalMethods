  RowBox[{
   RowBox[{"NewtoonMethod", "[", 
    RowBox[{"f_", ",", "x0_", ",", "epselon_"}], "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"n", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"x1", "=", "x0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"x2", "=", 
      RowBox[{"x1", "-", 
       RowBox[{
        RowBox[{"f", "[", "x1", "]"}], "/", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"f", "[", "x", "]"}], ",", "x"}], "]"}], "/.", 
          RowBox[{"x", "\[Rule]", "x1"}]}], ")"}]}]}]}], ";", 
