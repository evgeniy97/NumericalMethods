Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"BisecMethod", "[", 
    RowBox[{"f_", ",", "A_", ",", "B_", ",", "epselon_"}], "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"b", "=", "B"}], ";", 
     RowBox[{"a", "=", "A"}], ";", 
     RowBox[{"n", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"b", "-", "a"}], ")"}], "\[GreaterEqual]", 
        RowBox[{"2", " ", "epselon"}]}], ",", 
       RowBox[{
        RowBox[{"y", "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"a", "+", "b"}], ")"}], "/", "2"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"f", "[", "y", "]"}], " ", 
             RowBox[{"f", "[", "a", "]"}]}], "<", "0"}], ",", 
           RowBox[{"b", "=", "y"}], ",", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"f", "[", "y", "]"}], " ", 
               RowBox[{"f", "[", "b", "]"}]}], "<", "0"}], ",", 
             RowBox[{"a", "=", "y"}]}], "]"}]}], "]"}], " ", 
         RowBox[{"n", "++"}]}], ";"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"y", "//", "N"}], ",", "n"}], "}"}], "]"}]}], ")"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.7256828422482147`*^9, 3.7256828762017307`*^9}}],

